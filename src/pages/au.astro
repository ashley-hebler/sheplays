---
import PageHeader from '../components/PageHeader.astro';
import Layout from '../layouts/Layout.astro';
import GameList from '../components/GameList.astro';
import Header from '../components/Header.astro';
import { auAPI } from '../config';


const games = await fetch(auAPI);
const gameData = await games.json();
const gamesResults = await gameData.results;

---

<Layout title="Watch women's sports">
	<main>
		<Header />
		<PageHeader title="Athletes Unlimited" sport="softball" />
		<GameList gameList={gamesResults} compact={true} />
	</main>
</Layout>


